pipeline {
    agent any
    options {
        timeout(time: 1, unit: 'HOURS') 

    }
    environment {
    	PRIMARY_BRANCH = 'main'
	FEATURE_BRANCH_DEPLOYMENT = 'true'
	TARGET_BRANCH = 'dev'
    }
    stages {
        stage('first stage') {
            steps {
		sh 'python3 --version'
            }
        }
	stage('second stage') {
            steps {
                echo 'we want to Build hello_world example file'
			sh ' python3 hello.py'
			}
		}
    }
}

