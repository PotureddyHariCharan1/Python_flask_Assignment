pipeline {
    agent any
    options {
        timeout(time: 1, unit: 'HOURS') 
    }
    stages {
        stage('first stage') {
            steps {
		sh 'python3 --version'
            }
        }
	stage('second stage') {
            steps {
                echo 'we want to Build hello_world example file'
			sh ' python3 hello.py'
			}
		}
    }
}

